{% block html_head_container %}
<!doctype html>
<!--[if lt IE 7]><html class="no-js ie ie6 lt-ie9 lt-ie8 lt-ie7" {{site.language_attributes}}> <![endif]-->
<!--[if IE 7]><html class="no-js ie ie7 lt-ie9 lt-ie8" {{site.language_attributes}}> <![endif]-->
<!--[if IE 8]><html class="no-js ie ie8 lt-ie9" {{site.language_attributes}}> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js" {{site.language_attributes}}> <!--<![endif]-->
<head>
  <meta charset="{{site.charset}}" />
  <title>
    {% if wp_title %}
    {{ wp_title }} - {{ site.name }}
    {% else %}
    {{ site.name }}
    {% endif %}
  </title>
  <link rel="stylesheet" href="{{site.theme.link}}/assets/css/application.css" type="text/css" media="screen" />
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  {# Meta Data for SEO/Social Media #}
  <meta name="title" content="Global Docs" />
  <meta itemprop="name" content="Global Docs" />
  <meta property="og:title" content="Global Docs" />
  <meta property="og:description" content="{{site.description}}" />
  <meta property="og:image" content="http://globaldocs.us/wp-content/uploads/fbrfg/android-chrome-192x192.png" />
  <meta name="application-name" content="Global Docs">
  <meta name="description" content="{{site.description}}">
  <meta name="keywords" content="Global Docs, Document Legalization, USDA Certificate Reissue, Apostille,  Authentication, Documents, DC, International, Business,  Commercial, Educational">

  {# Favicons/Touch Icons
  If you dont use RealFaviconWP plugin, use the partial:
  {% include 'partials/favicon.twig' %} #}

  {{function('wp_head')}}
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
      <link rel="stylesheet" href="css/ie.css">
    <![endif]-->

</head>
{% endblock %}

<body class="{{body_class}}">
  {#
  GLOBAL SITE HEADER
  ------------------------#}
  {% block header %}
  {% include 'partials/header.twig' with {'classes': ''} %}
  {% endblock %}

  {#
  SCREEN CONTENT
  ------------------------#}
  {% block content %}
  {% endblock %}
  
  {#
  FOOTER
  ------------------------#}
  {% block footer %}
  {% include 'partials/footer.twig' %}
  {% endblock %}

  {#
  SCRIPTS
  ------------------------#}

  {% if isHomepage %}
  {% endif %}
  
  </body>
  </html>