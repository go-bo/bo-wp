{% extends "base.twig" %}

{% block content %}
<div class="PageHeader">
  <h1 class="PageHeader-title">{{post.title}}</h1>
  <h4>Categories</h4>
  <ul>
    {% for category in insight_categories %}
      <li><a href="{{category.link}}">{{category.name}}</a></li>
    {% endfor %}
  </ul>
</div>
<div class="container">
  
  {% for post in posts %}
    <h3>{{post.title}}</h3>
  {% endfor %}

</div>
{% endblock %}
